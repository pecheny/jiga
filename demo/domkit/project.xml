<?xml version="1.0" encoding="utf-8"?>
<project>

    <meta title="Sample" package="shoo.fancy.DkitDemo" version="1.0.0" company="shoo"/>
    <app main="DkitDemo" />

    <window antialiasing="4"/>
    <window background="#303030" fps="60" />
    <window background="#303030" fps="60" width="1280" height="760" unless="html5"/>
    <source path="src"/>
    <source path="../../src"/>

    <haxelib name="openfl"/>
    <haxelib name="tracer"/>
    <haxelib name="ginp"/>
    <haxelib name="taxis"/>
    <haxelib name="fancy"/>
    <haxelib name="alayout"/>
    <haxelib name="hxGeomAlgo"/>
    <haxelib name="json2object"/>
    <haxelib name="hscript"/>
    <haxelib name="domkit"/>

    <haxeflag name="--macro" value='domkit.Macros.registerComponentsPath("fancy.domkit.Dkit.$Dkit")'/>
</project>
